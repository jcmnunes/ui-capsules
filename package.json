{
  "name": "@binarycapsule/ui-capsules",
  "version": "0.0.0-development",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "assets",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build:app": "react-scripts build && echo '/* /index.html  200' | cat >build/_redirects",
    "test": "react-scripts test --passWithNoTests",
    "test:nowatch": "CI=true npm run test",
    "eject": "react-scripts eject",
    "lint": "eslint ./src/**/*.{js,jsx}",
    "format": "pretty-quick --staged",
    "clean": "rm -rf dist && rm -rf assets",
    "types": "tsc -p tsconfig.json",
    "build": "npm run clean && tsc -p tsconfig.prod.json && cp -rf src/lib/styles assets",
    "validate": "npm-run-all --parallel types format lint build",
    "pack": "npm run build && npm pack",
    "semantic-release": "semantic-release",
    "commit": "git-cz"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test:nowatch && npm run types && npm run build && lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": "eslint --fix",
    "*.+(js|jsx|ts|tsx|json|md)": "prettier --write"
  },
  "dependencies": {
    "@tippyjs/react": "^4.2.0",
    "downshift": "^6.0.6",
    "react-modal": "^3.11.2",
    "react-select": "^3.1.0",
    "react-switch": "^5.0.1",
    "react-toast-notifications": "^2.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.12.1",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@styled-system/should-forward-prop": "^5.1.5",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.1",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-modal": "^3.10.6",
    "@types/react-router-dom": "^5.1.6",
    "@types/react-select": "^3.0.23",
    "@types/react-toast-notifications": "^2.4.0",
    "@types/styled-system": "^5.1.10",
    "@types/styled-system__should-forward-prop": "^5.1.2",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "babel-preset-react-app": "^10.0.0",
    "commitizen": "^4.2.2",
    "csstype": "^3.0.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^17.0.1",
    "react-element-to-jsx-string": "^14.3.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.0",
    "semantic-release": "^17.3.0",
    "styled-system": "^5.1.5",
    "typescript": "^4.0.5"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "@emotion/react": "^11.1.2",
    "@emotion/styled": "^11.0.0",
    "@styled-system/should-forward-prop": "^5.1.5",
    "@types/styled-system": "^5.1.10",
    "styled-system": "^5.1.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babelMacros": {
    "styledComponents": {
      "pure": true
    }
  },
  "description": "UI kit",
  "repository": {
    "type": "git",
    "url": "https://github.com/jcmnunes/ui-capsules.git"
  },
  "keywords": [
    "ui",
    "kit",
    "components",
    "react"
  ],
  "author": "Jose C. Nunes <jn@josenunes.xyz> (https://josenunes.xyz/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jcmnunes/ui-capsules/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
