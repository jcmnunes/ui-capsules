{
  "name": "@binarycapsule/ui-capsules",
  "version": "0.0.0-development",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "assets",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build:app": "react-scripts build && echo '/* /index.html  200' | cat >build/_redirects",
    "test": "react-scripts test --passWithNoTests",
    "test:nowatch": "CI=true npm run test",
    "eject": "react-scripts eject",
    "lint": "eslint ./src/**/*.{js,jsx}",
    "format": "pretty-quick --staged",
    "clean": "rm -rf dist && rm -rf assets",
    "types": "tsc -p tsconfig.json",
    "build": "npm run clean && tsc -p tsconfig.prod.json && cp -rf src/lib/styles assets && cp -rf src/lib/setup.js dist",
    "validate": "npm-run-all --parallel types format lint build",
    "pack": "npm run build && npm pack",
    "semantic-release": "semantic-release",
    "commit": "git-cz"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test:nowatch && npm run types && npm run build && lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": "eslint --fix",
    "*.+(js|jsx|ts|tsx|json|md)": "prettier --write"
  },
  "dependencies": {
    "@reach/auto-id": "^0.16.0",
    "@tippyjs/react": "^4.2.3",
    "downshift": "^6.1.0",
    "focus-visible": "^5.2.0",
    "react-modal": "^3.12.1",
    "react-popper": "^2.2.4",
    "react-select": "^4.3.1",
    "react-switch": "^6.0.0",
    "react-toastify": "^7.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.13.9",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@stitches/react": "^1.2.5",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-modal": "^3.12.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-select": "^4.0.17",
    "@types/react-toast-notifications": "^2.4.0",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "babel-preset-react-app": "^10.0.0",
    "commitizen": "^4.2.4",
    "csstype": "^3.0.7",
    "cz-conventional-changelog": "^3.3.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^5.1.3",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-copy-to-clipboard": "^5.0.3",
    "react-dom": "^17.0.1",
    "react-element-to-jsx-string": "^14.3.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "semantic-release": "^17.4.1",
    "typescript": "^4.2.2"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "@stitches/react": "^1.2.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "description": "UI kit",
  "repository": {
    "type": "git",
    "url": "https://github.com/jcmnunes/ui-capsules.git"
  },
  "keywords": [
    "ui",
    "kit",
    "components",
    "react"
  ],
  "author": "Jose C. Nunes <josenunes@hey.com> (https://josenunes.dev/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jcmnunes/ui-capsules/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
